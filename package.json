{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "Authentication service for AudioBook backend",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development node dist/index.js",
    "start:staging": "NODE_ENV=staging node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:staging": "NODE_ENV=staging nodemon src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:seed": "ts-node prisma/seed.ts",
    "generate-keys": "ts-node scripts/generate-keys.ts"
  },
  "keywords": ["auth", "jwt", "oauth", "typescript", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "argon2": "^0.31.2",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.12",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "crypto": "^1.0.1",
    "node-forge": "^1.3.1",
    "amqplib": "^0.10.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/cors": "^2.8.17",
    "@types/cookie-parser": "^1.4.6",
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.8",
    "@types/node-forge": "^1.3.11",
    "@types/amqplib": "^0.10.4",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "ts-jest": "^29.1.1"
  }
}
